	mesh							models/characters/marine_kanestrogg/ks_npc.md5mesh
	
	channel torso	( *waist )
	channel legs	( *origin *hips -*waist )
	
	anim box_blaster				models/characters/marine_kanestrogg/box_blaster.md5anim
