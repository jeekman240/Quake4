	anim	GLA_v2_walk_back.ma				-dest gla_walk_back
	anim	GLA_v2_walk_left.ma				-range 1 34 -dest gla_walk_left
	anim	GLA_v2_walk_right.ma			-range 1 34 -dest gla_walk_right
