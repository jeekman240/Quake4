model model_monster_heavy_hovertank
{
	mesh							models/monsters/heavyhovertank/mesh.md5mesh

	channel	torso					( *waist )
	channel	legs					( *tank_base origin )

//	offset							( 0 0 -24 )

	anim idle						models/monsters/heavyhovertank/idle.md5anim
	anim af_pose					models/monsters/heavyhovertank/af_pose.md5anim

	anim blaster_to_rocket			models/monsters/heavyhovertank/idle1_to_idle2.md5anim
	{
		frame 1						sound_weapon		snd_rocket_ready
		rate 2.0
	}
	anim rocket_to_blaster			models/monsters/heavyhovertank/idle2_to_idle1.md5anim
	{
		frame 1						sound_weapon		snd_rocket_stow
		rate 2.0
	}

	anim run						models/monsters/heavyhovertank/idle.md5anim

	anim pain1						models/monsters/heavyhovertank/pain_std.md5anim
	{
		frame 1						sound_voice		snd_pain
	}
	anim pain2						models/monsters/heavyhovertank/pain_std2.md5anim
	{
		frame 1						sound_voice		snd_pain
	}
	anim pain_concussive			models/monsters/heavyhovertank/pain_con.md5anim
	{
		frame 1						sound_voice		snd_pain
	}
	anim pain_stream_start			models/monsters/heavyhovertank/pain_strm_start.md5anim
	{
		frame 1						sound_voice		snd_pain
	}
	anim pain_stream				models/monsters/heavyhovertank/pain_strm_loop.md5anim
	anim pain_stream_end			models/monsters/heavyhovertank/pain_strm_end.md5anim

	anim melee_attack				models/monsters/heavyhovertank/melee.md5anim
	{
		frame 1						sound_weapon				snd_melee_swing
		frame 17					fx					fx_swipe			lft_wrst
		frame 20					ai_attack_melee 	melee
		frame 15					sound_voice			snd_grunt
	}
	anim melee_attack2				models/monsters/heavyhovertank/melee2.md5anim
	{
		frame 1						sound_weapon				snd_melee_swing
		frame 12					fx					fx_swipe			lft_wrst
		frame 18					ai_attack_melee 	melee_right
		frame 11					sound_voice			snd_grunt
	}
	anim blaster_1_preshoot			models/monsters/heavyhovertank/preshoot.md5anim
	{
		rate 2.0
	}
	anim blaster_1_postshoot		models/monsters/heavyhovertank/postshoot.md5anim
	{
		rate 2.0
	}
	anim blaster_1_fire				models/monsters/heavyhovertank/machine_gun.md5anim
	{
		rate 3
   		frame 1						ai_attack			blaster				machine_gun_barrel
   		frame 1						fx					fx_blaster_flash	machine_gun_barrel
   		frame 1						sound_weapon				snd_weapon_fire
	}
	anim blaster_2_preshoot			models/monsters/heavyhovertank/preshoot_2.md5anim
	{
		rate 2.0
	}
	anim blaster_2_postshoot		models/monsters/heavyhovertank/postshoot_2.md5anim
	{
		rate 2.0
	}
	anim blaster_2_fire				models/monsters/heavyhovertank/machine_gun_2.md5anim
	{
		rate 3
   		frame 1						ai_attack			blaster				machine_gun_barrel
   		frame 1						fx					fx_blaster_flash	machine_gun_barrel
   		frame 1						sound_weapon				snd_weapon_fire
	}

	anim rocket_idle				models/monsters/heavyhovertank/idle2.md5anim
	anim rocket_run					models/monsters/heavyhovertank/idle2.md5anim
	anim rocket_range_attack		models/monsters/heavyhovertank/big_gun.md5anim
	{
		rate 2.0
		frame 57					ai_attack			rocket				muzzle_flash
		frame 57					fx					fx_rocket_flash		muzzle_flash
	}
	
	anim wake						models/monsters/heavyhovertank/wake.md5anim
	{
		frame 1						speak			lipsync_sight
	}
	anim wake2						models/monsters/heavyhovertank/wake2.md5anim
	{
		frame 1						speak			lipsync_sight
	}

	anim evade_left1				models/monsters/heavyhovertank/evade_left1.md5anim
	{
		frame 1						sound_weapon				snd_strafe
	}
	anim evade_right1				models/monsters/heavyhovertank/evade_right1.md5anim
	{
		frame 1						sound_weapon				snd_strafe
	}
}

