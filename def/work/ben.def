export ben {
	options -prefix GUN_ -align ALL -scale .4
	mesh	models/gunner/walk9.ma -dest models/md5/meshes/gunner
	anim	models/gunner/walk9.ma -dest models/md5/anims/gunner/walk
	anim	models/gunner/idle.ma -dest models/md5/anims/gunner/idle
	anim	models/gunner/run16.ma -dest models/md5/anims/gunner/run
	anim	models/gunner/punch7.ma -dest models/md5/anims/gunner/punch
	anim	models/gunner/backhand.ma -dest models/md5/anims/gunner/backhand
	anim	models/gunner/attack.ma -dest models/md5/anims/gunner/attack
	anim	models/gunner/walk_attack.ma -dest models/md5/anims/gunner/walk_attack
	anim	models/gunner/strafe2.ma -dest models/md5/anims/gunner/strafe
	anim	models/gunner/pain1.ma -dest models/md5/anims/gunner/pain1





}
