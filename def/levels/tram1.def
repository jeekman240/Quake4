TRAM1_grunt_door
***********************************************************************/
export tram1_grunt_door
{		
	options -scale .3937 -prefix DOOOR_ -align ALL 
         
	mesh	work/models/mapobjects/strogg/tram/grunt_door/door_idle.ma	  						-dest models/mapobjects/tram/door/mesh
	anim	work/models/mapobjects/strogg/tram/grunt_door/door_idle.ma	  						-dest models/mapobjects/tram/door/idle
	anim	work/models/monsters/grunt/animations/tram_grunt_intro2.ma	  						-dest models/mapobjects/tram/door/grunt_tears_door
	
}

entityDef tram1_grunt_door
{
	"model"							"model_tram1_grunt_door"
	"spawnclass"					"idAnimated"
	
	"editor_color"					"0 .5 .25"
	"editor_mins"					"-8 -8 0"
	"editor_maxs"					"8 8 16"
	"editor_usage"					"Animated door pulled open by grunt in tram1"
	"editor_menu"					"level"

	//"animate_z"						"1"
	//"floats"						"1"
	"anim"							"idle"
	"start_anim"					"idle"	
	
	"snd_tear"						"ambient_metal_stress_violent"
	"snd_break1"					"generic_small_metal_break"
	"snd_break2"					"generic_large_metal_break"
}

model model_tram1_grunt_door
{
	mesh												models/mapobjects/tram/door/mesh.md5mesh
	anim idle											models/mapobjects/tram/door/idle.md5anim
	anim grunt_tears									models/mapobjects/tram/door/grunt_tears_door.md5anim
	{
		frame	8					sound	snd_tear
		frame	16					sound	snd_break1
		frame	30					sound	snd_break2
		rate 1.1
	}
}	


